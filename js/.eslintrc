{
  "root": true,
  "extends": "eslint:recommended",
  "rules": {
    "indent": [ 2, 2, {"SwitchCase": 1}],
    "linebreak-style": [ 0, "windows"  ],
    "semi": [ 2, "always" ],
    "no-unused-vars": 0,
    "no-console": 0
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jquery": true
  },
  "parserOptions": {
      "sourceType": "module"
  }
}
